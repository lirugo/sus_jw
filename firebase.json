{
    "flutter": {
        "platforms": {
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "susjw-20b2d",
                    "configurations": {
                        "web": "1:672717485011:web:b4839b25cb16f5509d7526"
                    }
                }
            }
        }
    },
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "**",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate"
                    },
                    {
                        "key": "Pragma",
                        "value": "no-cache"
                    },
                    {
                        "key": "Expires",
                        "value": "0"
                    }
                ]
            }
        ]
    }
}